  "devDependencies": {
    "@types/node": "20.13.0",
    "@types/node-fetch": "2.6.12",
    "@types/tough-cookie": "4.0.5",
    "@typescript-eslint/eslint-plugin": "8.18.2",
    "@typescript-eslint/parser": "8.18.2",
    "eslint": "9.17.0",
    "homebridge": "1.8.5",
    "nodemon": "3.1.9",
    "rimraf": "6.0.1",
    "ts-node": "10.9.2",
    "typescript": "5.7.2",
    "typescript-eslint": "8.18.2"
  }
