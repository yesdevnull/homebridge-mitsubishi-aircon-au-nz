    "url": "https://github.com/yesdevnull/homebridge-mitsubishi-aircon-au-nz/issues"
  },
  "type": "module",
  "engines": {
    "homebridge": "^1.6.0 || ^2.0.0-beta.0 || ^2.0.0",
    "node": "^18.20.4 || ^20.15.1 || ^22.12.0"
  },
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint . --max-warnings=0",
